{
  "name": "starter-wdio",
  "version": "1.0.0",
  "description": "UI Test automation framework based on WebdriverIO",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf .tsbuid && rimraf allure-results && rimraf junit-results",
    "clean-report": "allure generate --clean ./allure-results",
    "wdio": "npm run clean && wdio run test/wdio.conf.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnaVictorio/Appium.git"
  },
  "keywords": [
    "wdio",
    "cucumber",
    "gherkin",
    "typescript"
  ],
  "author": "Ana Victorio",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AnaVictorio/Appium/issues"
  },
  "homepage": "https://github.com/AnaVictorio/Appium#readme",
  "devDependencies": {
    "@types/jquery": "^3.5.14",
    "@wdio/allure-reporter": "^7.26.0",
    "@wdio/cli": "^7.27.0",
    "@wdio/cucumber-framework": "^7.27.0",
    "@wdio/junit-reporter": "^7.26.0",
    "@wdio/local-runner": "^7.27.0",
    "@wdio/spec-reporter": "^7.26.0",
    "chromedriver": "^107.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "wdio-chromedriver-service": "^8.0.0"
  },
  "dependencies": {
    "rimraf": "^3.0.2"
  }
